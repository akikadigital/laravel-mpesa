{
    "name": "akika/laravel-mpesa",
    "description": "A cool Mpesa Daraja package for Larave supporting Laravel version 5 and above.",
    "homepage": "https://github.com/Akika-Digital/laravel-mpesa",
    "type": "library",
    "keywords": [
        "mpesa",
        "daraja",
        "laravel",
        "payment",
        "Akika",
        "Digital"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^8.0 | ^9.0 | ^10.0 | ^11.0",
		"illuminate/http":"^8.0 | ^9.0 | ^10.0 | ^11.0",
		"guzzlehttp/guzzle": "^7.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Akika\\LaravelMpesa\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Felix Nyabwari",
            "email": "fenicfelix@gmail.com"
        }
    ],
    "extra": {
		"laravel": {
			"providers": [
				"Akika\\LaravelMpesa\\MpesaServiceProvider"
			],
			"aliases": {
				"Mpesa": "Akika\\LaravelMpesa\\Facades\\Mpesa"
			}
		}
  	}
}
